[req]
default_md = sha256
prompt = no
req_extensions = v3_ext
distinguished_name = req_distinguished_name

[req_distinguished_name]
CN = enode-0.enode.marklogic-tlsednode.svc.cluster.local
OU=MarkLogic

[v3_ext]
keyUsage = critical,digitalSignature,keyEncipherment
extendedKeyUsage = critical,serverAuth,clientAuth
subjectAltName = DNS:localhost, DNS:enode-0.enode.marklogic-tlsednode.svc.cluster.local